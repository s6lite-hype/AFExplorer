<Project Sdk="Microsoft.NET.Sdk">

  <ItemGroup>
    <ProjectReference Include="..\ProjectManager\ProjectManager.csproj" />
    <None Update="platform-tools\**\*">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <Content Include="icl.ico">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
